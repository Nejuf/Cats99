  <form action="<%= cat_rental_requests_url %>", method="post">
  <label for="cat_name">Name</label>
  <select name="cat_rental_requests[cat_id]" id="cat_id">
    <option></option>
    <% @cats.each do |cat| %>
    <option value="<%= cat.id %>"> <%= cat.name %> </option>
    <%end%>
  </select>
  <br>
  <br>
  <label for="start_date">Start Date</label>
  <input type="date" id="start_date" name="cat_rental_requests[start_date]">
  <br>
  <label for="end_date">End Date</label>
  <input type="date" id="end_date" name="cat_rental_requests[end_date]">
  <input type="submit" value="Rent cat">
