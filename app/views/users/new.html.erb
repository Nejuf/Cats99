<h1>Create User</h1>
<form action=<%= users_url %> method="post" accept-charset="utf-8">
	<input type="hidden" name="authenticity_token" value=<%=form_authenticity_token%>>
	<label for="user_name">Username</label><input type="text" name="user[user_name]" id="user_name">
	<label for="password">Password</label><input type="text" name="user[password]" id="password">
	<p><input type="submit" value="Create user"></p>
</form>